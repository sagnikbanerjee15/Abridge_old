abridge \
--random \
--inputabrfilenames $ROOT_DIRECTORY/developing_abridge/SRR13711355_PE.sam.abridge.br  \
--positions 1:5000-7000 \
--genome /project/maizegdb/sagnik/data/ARATH/genome/Arabidopsis_thaliana.TAIR10.dna.toplevel.fa \
--output_directory $ROOT_DIRECTORY/developing_abridge/decompress \
) \
1> $ROOT_DIRECTORY/developing_abridge/tests_random.output \
2> $ROOT_DIRECTORY/developing_abridge/tests_random.error &



TESTS WITH ONE RNA-SEQ SAMPLE
=============================
(/usr/bin/time --verbose  \
abridge \
--keep_intermediate_error_files \
--compress \
--cpu $CPU \
--both \
--save_all_quality_scores \
--save_exact_quality_scores \
--run_diagnostics \
--genome /project/maizegdb/sagnik/data/ARATH/genome/Arabidopsis_thaliana.TAIR10.dna.toplevel.fa  \
--inputsamfilenames $ROOT_DIRECTORY/developing_abridge/SRR13711353_PE.sam \
--output_directory $ROOT_DIRECTORY/developing_abridge/save_all_quality_scores_save_exact_quality_scores ) \
1> $ROOT_DIRECTORY/developing_abridge/save_all_quality_scores_save_exact_quality_scores/tests_compress.output \
2> $ROOT_DIRECTORY/developing_abridge/save_all_quality_scores_save_exact_quality_scores/tests_compress.error &

(/usr/bin/time --verbose  \
abridge \
--keep_intermediate_error_files \
--compress \
--cpu $CPU \
--both \
--save_all_quality_scores \
--run_diagnostics \
--genome /project/maizegdb/sagnik/data/ARATH/genome/Arabidopsis_thaliana.TAIR10.dna.toplevel.fa  \
--inputsamfilenames $ROOT_DIRECTORY/developing_abridge/SRR13711353_PE.sam \
--output_directory $ROOT_DIRECTORY/developing_abridge/save_all_quality_scores_save_approx_quality_scores ) \
1> $ROOT_DIRECTORY/developing_abridge/save_all_quality_scores_save_approx_quality_scores/tests_compress.output \
2> $ROOT_DIRECTORY/developing_abridge/save_all_quality_scores_save_approx_quality_scores/tests_compress.error &


(/usr/bin/time --verbose  \
abridge \
--keep_intermediate_error_files \
--compress \
--cpu $CPU \
--both \
--run_diagnostics \
--genome /project/maizegdb/sagnik/data/ARATH/genome/Arabidopsis_thaliana.TAIR10.dna.toplevel.fa  \
--inputsamfilenames $ROOT_DIRECTORY/developing_abridge/SRR13711353_PE.sam \
--output_directory $ROOT_DIRECTORY/developing_abridge/save_only_indels_save_qual_scores ) \
1> $ROOT_DIRECTORY/developing_abridge/save_only_indels_save_qual_scores/tests_compress.output \
2> $ROOT_DIRECTORY/developing_abridge/save_only_indels_save_qual_scores/tests_compress.error &


(/usr/bin/time --verbose  \
abridge \
--keep_intermediate_error_files \
--compress \
--cpu $CPU \
--both \
--run_diagnostics \
--ignore_quality_scores \
--genome /project/maizegdb/sagnik/data/ARATH/genome/Arabidopsis_thaliana.TAIR10.dna.toplevel.fa  \
--inputsamfilenames $ROOT_DIRECTORY/developing_abridge/SRR13711353_PE.sam \
--output_directory $ROOT_DIRECTORY/developing_abridge/save_only_indels_nosave_qual_scores ) \
1> $ROOT_DIRECTORY/developing_abridge/save_only_indels_nosave_qual_scores/tests_compress.output \
2> $ROOT_DIRECTORY/developing_abridge/save_only_indels_nosave_qual_scores/tests_compress.error &

(/usr/bin/time --verbose  \
abridge \
--keep_intermediate_error_files \
--compress \
--cpu $CPU \
--both \
--ignore_soft_clippings \
--ignore_mismatches \
--genome /project/maizegdb/sagnik/data/ARATH/genome/Arabidopsis_thaliana.TAIR10.dna.toplevel.fa  \
--inputsamfilenames $ROOT_DIRECTORY/developing_abridge/SRR13711353_PE.sam \
--output_directory $ROOT_DIRECTORY/developing_abridge/ignore_soft_clippings_ignore_mismatches ) \
1> $ROOT_DIRECTORY/developing_abridge/ignore_soft_clippings_ignore_mismatches/tests_compress.output \
2> $ROOT_DIRECTORY/developing_abridge/ignore_soft_clippings_ignore_mismatches/tests_compress.error &



COVERAGE GENERATION
===================
(/usr/bin/time --verbose  \
abridge \
--keep_intermediate_error_files \
--compress \
--cpu $CPU \
--generate_overlapping_coverage \
--genome /project/maizegdb/sagnik/data/ARATH/genome/Arabidopsis_thaliana.TAIR10.dna.toplevel.fa  \
--inputabrfilenames $ROOT_DIRECTORY/developing_abridge/save_all_quality_scores_save_approx_quality_scores/SRR13711353_PE.sam.abridge.7z \
--output_directory $ROOT_DIRECTORY/developing_abridge/save_all_quality_scores_save_approx_quality_scores ) \
1> $ROOT_DIRECTORY/developing_abridge/save_all_quality_scores_save_approx_quality_scores/tests_non_overlapping_cov_generation.output \
2> $ROOT_DIRECTORY/developing_abridge/save_all_quality_scores_save_approx_quality_scores/tests_non_overlapping_cov_generation.error &


