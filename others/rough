
CPU=1
===============================
SINGLE ENDED COMPRESSION MKDIRS
===============================

mkdir -p \
/90daydata/maizegdb/sagnik/ABRIDGE/developing_abridge/compress_save_all_quality_scores_save_exact_quality_scores_SE \
/90daydata/maizegdb/sagnik/ABRIDGE/developing_abridge/compress_save_all_quality_scores_save_approx_quality_scores_SE \
/90daydata/maizegdb/sagnik/ABRIDGE/developing_abridge/compress_save_only_indels_save_qual_scores_SE \
/90daydata/maizegdb/sagnik/ABRIDGE/developing_abridge/compress_save_only_indels_nosave_qual_scores_SE \
/90daydata/maizegdb/sagnik/ABRIDGE/developing_abridge/compress_ignore_soft_clippings_ignore_mismatches_SE \
/90daydata/maizegdb/sagnik/ABRIDGE/developing_abridge/compress_ignore_soft_clippings_ignore_mismatches_ignore_quality_scores_SE

==========================
SINGLE ENDED COMPRESSION 1
==========================
(/usr/bin/time --verbose  \
abridge \
--keep_intermediate_error_files \
--compress \
--cpu $CPU \
--save_all_quality_scores \
--save_exact_quality_scores \
--fast \
--genome /90daydata/maizegdb/sagnik/data/ARATH/genome/Arabidopsis_thaliana.TAIR10.dna.toplevel.fa  \
--inputsamfilenames /project/maizegdb/sagnik/ABRIDGE/developing_abridge/SRR13711353_SE.sam \
--output_directory /90daydata/maizegdb/sagnik/ABRIDGE/developing_abridge/compress_save_all_quality_scores_save_exact_quality_scores_SE) \
1> /90daydata/maizegdb/sagnik/ABRIDGE/developing_abridge/compress_save_all_quality_scores_save_exact_quality_scores_SE.output \
2> /90daydata/maizegdb/sagnik/ABRIDGE/developing_abridge/compress_save_all_quality_scores_save_exact_quality_scores_SE.error &

