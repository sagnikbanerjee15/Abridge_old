
CPU=1
SINGLE ENDED
============

(/usr/bin/time --verbose  \
abridge \
--keep_intermediate_error_files \
--compress \
--cpu $CPU \
--save_all_quality_scores \
--save_exact_quality_scores \
--skip_compression \
--fast \
--genome /project/maizegdb/sagnik/data/ARATH/genome/Arabidopsis_thaliana.TAIR10.dna.toplevel.fa  \
--inputsamfilenames /project/maizegdb/sagnik/ABRIDGE/developing_abridge/SRR13711353_SE.sam \
--output_directory /project/maizegdb/sagnik/ABRIDGE/developing_abridge/save_all_quality_scores_save_exact_quality_scores) \
1> /project/maizegdb/sagnik/ABRIDGE/developing_abridge/save_all_quality_scores_save_exact_quality_scores.output \
2> /project/maizegdb/sagnik/ABRIDGE/developing_abridge/save_all_quality_scores_save_exact_quality_scores.error &

(/usr/bin/time --verbose  \
